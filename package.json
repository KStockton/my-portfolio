{
  "name": "mks-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.14.3",
    "formik": "^1.5.8",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.4.1",
    "sass": "1.26.10",
    "shortid": "2.2.15",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "./node_modules/.bin/eslint src"
  },
  "eslintConfig": {
    "extends": "react-app"
